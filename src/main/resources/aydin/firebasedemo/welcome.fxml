<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemo.WelcomeController" alignment="CENTER" spacing="20.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Button fx:id="registerButton" text="Register" onAction="#registerButtonClicked" prefWidth="200"/>
        <Button fx:id="signInButton" text="Sign In" onAction="#signInButtonClicked" prefWidth="200"/>
    </children>
</VBox>
